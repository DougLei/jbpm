<?xml version="1.0" encoding="UTF-8"?>
<mapping-configuration>
	<!-- 流程类型表 -->
	<table name="BPM_RE_PROCTYPE" class="com.douglei.bpm.module.repository.type.ProcessType" forceCreateMode="create">
		<columns>
			<column name="ID" property="id" dataType="integer" primaryKey="true" length="20" />
			<column name="CODE" property="code" dataType="string" length="50" nullable="false" />
			<column name="NAME" property="name" dataType="nstring" length="30" nullable="false" />
			<column name="CREATE_USER" property="createUser" dataType="char" length="50" />
			<column name="CREATE_DATE" property="createDate" dataType="date" nullable="false" />
			<column name="UPDATE_USER" property="updateUser" dataType="char" length="50" />
			<column name="UPDATE_DATE" property="updateDate" dataType="date" nullable="false" />
		</columns>
		<primaryKeyHandler type="sequence"></primaryKeyHandler>
	</table>
</mapping-configuration>