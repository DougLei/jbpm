<?xml version="1.0" encoding="UTF-8"?>
<mapping-configuration>
	<!-- 流程历史调度表 -->
	<table name="BPM_HI_DISPATCH" class="com.douglei.bpm.module.history.task.HistoryDispatch" createMode="create" strict="true">
		<columns>
			<column name="ID" property="id" dataType="number" primaryKey="true" length="10" />
			<column name="TASKINST_ID" property="taskinstId" dataType="char" length="36" nullable="false"/>
			<column name="CHAIN_ID" property="chainId" dataType="number" length="3" nullable="false" />
			<column name="USER_ID" property="userId" dataType="nstring" length="150" nullable="false" />
			<column name="REASON" property="reason" dataType="nstring" length="300" />
			<column name="DISPATCH_TIME" property="dispatchTime" dataType="datetime" nullable="false" />
		</columns>
		<primaryKeyHandler type="sequence"></primaryKeyHandler>
	</table>
</mapping-configuration>