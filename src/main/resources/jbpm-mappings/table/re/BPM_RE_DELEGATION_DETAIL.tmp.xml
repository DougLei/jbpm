<?xml version="1.0" encoding="UTF-8"?>
<mapping-configuration>
	<!-- 流程委托明细表 -->
	<table name="BPM_RE_DELEGATION_DETAIL" class="com.douglei.bpm.module.repository.delegation.DelegationDetail" createMode="create" strict="true">
		<columns>
			<column name="ID" property="id" dataType="number" length="10" />
			<column name="DELEGATION_ID" property="delegationId" dataType="number" length="10" nullable="false" />
			<column name="PROCDEF_CODE" property="procdefCode" dataType="string" length="50" nullable="false" />
			<column name="PROCDEF_VERSION" property="procdefVersion" dataType="string" length="50" />
		</columns>
		
		<constraints>
			<constraint type="PRIMARY_KEY" isAutoIncrement="true">
				<column name="ID" />
			</constraint>
		</constraints>
	</table>
</mapping-configuration>