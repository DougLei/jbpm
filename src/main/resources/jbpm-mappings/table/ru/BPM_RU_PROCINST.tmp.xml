<?xml version="1.0" encoding="UTF-8"?>
<mapping-configuration>
	<!-- 流程运行实例表 -->
	<table name="BPM_RU_PROCINST" class="com.douglei.bpm.module.runtime.instance.entity.ProcessRuntimeInstance" createMode="create" strict="true">
		<columns>
			<column name="ID" property="id" dataType="number" primaryKey="true" length="10" />
			<column name="PROCDEF_ID" property="procdefId" dataType="number" length="10" nullable="false"/>
			<column name="TITLE" property="title" dataType="nstring" length="200" nullable="false"/>
			<column name="BUSINESS_ID" property="businessId" dataType="string" length="100"/>
			<column name="STATE" property="state" dataType="number" length="1" defaultValue="0" nullable="false"/>
			<column name="START_USER" property="startUser" dataType="nstring" length="150" />
			<column name="START_TIME" property="startTime" dataType="datetime" nullable="false" />
		</columns>
		<primaryKeyHandler type="sequence"></primaryKeyHandler>
	</table>
</mapping-configuration>