<?xml version="1.0" encoding="UTF-8"?>
<mapping-configuration>
	<!-- 流程委托表 -->
	<table name="BPM_RE_DELEGATION" forceCreateMode="create">
		<columns>
			<column name="ID" property="id" dataType="integer" primaryKey="true" length="20" />
			<column name="CLIENT" property="client" dataType="char" length="50" nullable="false" />
			<column name="TRUSTEE" property="trustee" dataType="char" length="50" nullable="false" />
			<column name="START_DATE" property="startDate" dataType="date" nullable="false" />
			<column name="END_DATE" property="endDate" dataType="date" nullable="false" />
			<column name="REASON" property="reason" dataType="nstring" length="100" nullable="false" />
			<column name="CREATE_USER" property="createUser" dataType="char" length="50" />
			<column name="CREATE_DATE" property="createDate" dataType="date" nullable="false" />
			<column name="UPDATE_USER" property="updateUser" dataType="char" length="50" />
			<column name="UPDATE_DATE" property="updateDate" dataType="date" nullable="false" />
		</columns>
		<primaryKeyHandler type="sequence"></primaryKeyHandler>
	</table>
</mapping-configuration>