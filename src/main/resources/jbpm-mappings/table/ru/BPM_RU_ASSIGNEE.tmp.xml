<?xml version="1.0" encoding="UTF-8"?>
<mapping-configuration>
	<!-- 流程运行指派表 -->
	<table name="BPM_RU_ASSIGNEE" class="com.douglei.bpm.module.runtime.task.entity.Assignee" createMode="create" strict="true">
		<columns>
			<column name="ID" property="id" dataType="number" primaryKey="true" length="10" />
			<column name="TASK_ID" property="taskId" dataType="number" length="10" nullable="false"/>
			<column name="USER_ID" property="userId" dataType="nstring" length="150" nullable="false" />
			<column name="IS_HANDLING" property="isHandling" dataType="number" length="1" defaultValue="0"/>
			<column name="START_TIME" property="startTime" dataType="datetime"/>
		</columns>
		<primaryKeyHandler type="sequence"></primaryKeyHandler>
	</table>
</mapping-configuration>