<?xml version="1.0" encoding="UTF-8"?>
<mapping-configuration>
	<!-- 流程委托表 -->
	<table name="BPM_RE_DELEGATION" class="com.douglei.bpm.module.repository.delegation.ProcessDelegation" createMode="create" strict="true">
		<columns>
			<column name="ID" property="id" dataType="number" primaryKey="true" length="10" />
			<column name="CLIENT_ID" property="clientId" dataType="nstring" length="150" nullable="false" />
			<column name="TRUSTEE_ID" property="trusteeId" dataType="nstring" length="150" nullable="false" />
			<column name="START_DATE" property="startDate" dataType="datetime" nullable="false" />
			<column name="END_DATE" property="endDate" dataType="datetime" nullable="false" />
			<column name="REASON" property="reason" dataType="nstring" length="300" nullable="false" />
		</columns>
		<primaryKeyHandler type="sequence"></primaryKeyHandler>
	</table>
</mapping-configuration>