<?xml version="1.0" encoding="UTF-8"?>
<mapping-configuration>
	<!-- 流程定义表 -->
	<table name="BPM_RE_PROCDEF" forceCreateMode="create">
		<columns>
			<column name="ID" property="id" dataType="integer" primaryKey="true" length="20" />
			<column name="REF_TYPE_ID" property="refTypeId" dataType="integer" length="20" />
			<column name="MODE" property="mode" dataType="integer" length="1" defaultValue="1" />
			<column name="CODE" property="code" dataType="string" length="50" nullable="false" />
			<column name="VERSION" property="version" dataType="string" length="50" nullable="false" />
			<column name="VERSION_" property="version_" dataType="integer" length="3" nullable="false" />
			<column name="NAME" property="name" dataType="nstring" length="50" nullable="false" />
			<column name="ENABLED" property="enabled" dataType="integer" length="1" defaultValue="0" />
			<column name="DESCRIPTION" property="description" dataType="nstring" length="150" />
			<column name="CREATE_USER_ID" property="createUserId" dataType="char" length="50" nullable="false" />
			<column name="CREATE_DATE" property="createDate" dataType="date" nullable="false" />
			<column name="LAST_UPDATE_USER_ID" property="lastUpdateUserId" dataType="char" length="50" nullable="false" />
			<column name="LAST_UPDATE_DATE" property="lastUpdateDate" dataType="date" nullable="false" />
		</columns>
		<primaryKeyHandler type="sequence"></primaryKeyHandler>
	</table>
</mapping-configuration>