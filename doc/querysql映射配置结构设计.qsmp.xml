<?xml version="1.0" encoding="UTF-8"?>
<mapping-configuration>
	<!-- 配置资源名 -->
	<query-sql name="">
		<!-- 
			content中配置具体的select sql
			* append: 是否要追加条件
			* package: 是否需要包装成子查询
		 -->
		<content append="" package="">
		</content>
		
		<!-- 参数标准集合, 可为空, 即没有标准, 完全由调用方把控 -->
		<parameter-standards>
			<!-- 
				具体的参数标准
				* prefix: 列名的前缀, 可不填
				* name: 列名, 必填, 不能为空
				* dataType: 列的数据类型, 可不填, 默认为string, 目前可使用值有: string, 
				* required: 该参数是否必须, 可为空, 默认为false
			 -->
			<parameter-standard prefix="" name="" dataType="" required="">
				<!-- 
					参数支持的操作, 可配置多组
					* name: 操作名, 可不填, 默认为eq, 目前可使用值有: eq(等于), ne(不等于), ge(大于等于), gt(大于), le(小于等于), lt(小于), like, nlike(like取反), btn, nbtn(btn取反), in, nin(in取反), order(排序)
				 -->
				<operator name="" />
			</parameter-standard>		
		</parameter-standards>	
	</query-sql>
</mapping-configuration>