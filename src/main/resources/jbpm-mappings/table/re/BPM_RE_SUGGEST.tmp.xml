<?xml version="1.0" encoding="UTF-8"?>
<mapping-configuration>
	<!-- 流程常用意见表 -->
	<table name="BPM_RE_SUGGEST" forceCreateMode="create">
		<columns>
			<column name="ID" property="id" dataType="integer" primaryKey="true" length="20" />
			<column name="USER_" property="user_" dataType="char" length="50" />
			<column name="CONTENT" property="content" dataType="nstring" length="300" nullable="false" />
			<column name="CREATE_USER" property="createUser" dataType="char" length="50" />
			<column name="CREATE_DATE" property="createDate" dataType="date" />
			<column name="UPDATE_USER" property="updateUser" dataType="char" length="50" />
			<column name="UPDATE_DATE" property="updateDate" dataType="date" />
		</columns>
		<primaryKeyHandler type="sequence"></primaryKeyHandler>
	</table>
</mapping-configuration>