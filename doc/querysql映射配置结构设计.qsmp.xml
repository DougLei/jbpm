<?xml version="1.0" encoding="UTF-8"?>
<mapping-configuration>
	<!-- 配置资源名 -->
	<query-sql name="">
		<!-- 
			content中配置具体的select sql
			* package: 是否需要包装成子查询, 默认为true
			* append: 是否要追加条件, 默认为false, 如果package为true时, append的配置会被忽略
		 -->
		<content package="" append="">
		</content>
		
		<!-- 参数标准集合, 可为空, 即没有标准, 完全由调用方把控 -->
		<parameter-standards>
			<!-- 
				具体的参数标准, name不能重复, 否则会抛出异常
				* name: 列名, 必填, 不能为空
				* dataType: 列的数据类型, 可不填, 默认为string, 目前可使用值有: string, 
				* required: 该参数是否必须, 可为空, 默认为false
			 -->
			<parameter-standard name="" dataType="" required="">
				<!-- 
					参数支持的操作, 可配置多组; 配置的操作不能重复, 否则会抛出异常; 也可不配置, 即无限次使用所有操作
					* name: 操作名, 可不填, 默认为eq, 目前可使用值有: eq(等于), ne(不等于), ge(大于等于), gt(大于), le(小于等于), lt(小于), like, nlike(like取反), btn, nbtn(btn取反), in, nin(in取反), order(排序)
					* times: 该参数可使用此操作的次数, 可不配置, 默认值为1, 小于1时, 使用默认值, 配置不合法时会抛出异常
				 -->
				<operator name="" times="1"/>
			</parameter-standard>		
		</parameter-standards>	
	</query-sql>
</mapping-configuration>